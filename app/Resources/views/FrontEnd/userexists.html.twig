{% extends "base.html.twig" %}

{% block title %} TravelFlex | Privacy {% endblock %}

{% block body %}
    <header role="banner" id="fh5co-header">
    <div class="container">
        <!-- <div class="row"> -->
        <nav class="navbar navbar-default">
            <div class="navbar-header">
                <!-- Mobile Toggle Menu Button -->
                <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"><i></i></a>
                <a class="navbar-brand" href="{{ path('index') }}">TravelFlex</a> 
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li><a class="external" href="{{ path('index') }}" data-nav-section="home"><span>Home</span></a></li>
                    <li><a class="external" href="{{ path('index') }}#fh5co-work" data-nav-section="work"><span>Work</span></a></li>
                    <li><a class="external" href="{{ path('index') }}#fh5co-testimonials" data-nav-section="testimonials"><span>Testimonials</span></a></li>
                    <li><a class="external" href="{{ path('index') }}#fh5co-services" data-nav-section="services"><span>Services</span></a></li>
                    <li><a href="#" data-nav-section="contact"><span>Contact</span></a></li>
                    <li><a class="external" href="{{ path('terms') }}"><span>Terms</span></a></li>
                    <li><a class="external" href="{{ path('privacy') }}"><span>Privacy</span></a></li>
                    <li><a class="external" href="{{ path('unsubscribe') }}"><span>Unsubscribe</span></a></li>
                </ul>
            </div>
        </nav>
        <!-- </div> -->
    </div>
</header>
        
<section id="fh5co-home" data-section="home" style="background-image: url(../images/full_image_2.jpg);height: 10em;" data-stellar-background-ratio="0.5">
    <div class="gradient"></div>
        <div class="container">
            <div class="text-wrap">
                <div class="text-inner">
                    <div class="row">
                        <div class="col-md-12" style="margin-bottom: 3em;">
                            <h1 class="to-animate">See the World with TravelFlex</h1>
                            <!--<h2 class="to-animate">Another free HTML5 bootstrap template by <a href="http://freehtml5.co/" target="_blank" title="Free HTML5 Bootstrap Templates">FREEHTML5.co</a> released under <a href="http://creativecommons.org/licenses/by/3.0/" target="_blank">Creative Commons 3.0</a></h2>-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <div class="slant"></div>
</section>
<section>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 style="padding-bottom:10px;font-size:30px;">Hello {{ name|capitalize }} {{ lastname|capitalize }} <br> It seems you are already a member of OfficeJobsGuru.com family.</h2>
                    <div style="text-align:justify;">
                        <h3 style="text-align: center;">If you wish to unsubscriber, please use link below and complete 2 step unsubscription process. In case of any questions, feel free to reach out 
                        to us using contact form below or sending an email to support@travelflex.com
                        </h3>
                    </div>	
                </div>
            </div>
            <!-- /.row -->
        </div>
</section>
{% endblock %}
